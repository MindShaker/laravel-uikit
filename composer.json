{
    "name": "mindshaker/laravel-uikit",
    "description": "UIkit starter template for Laravel 10",
    "minimum-stability": "stable",
    "keywords": [
        "laravel",
        "uikit",
        "breeze"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Fletcher",
            "email": "matt@torrix.uk"
        },
        {
            "name": "Miguel Neves",
            "email": "miguel.neves@mind-shaker.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "illuminate/console": "^10.0|^11",
        "illuminate/pagination": "^10.0|^11",
        "illuminate/filesystem": "^10.0|^11",
        "illuminate/support": "^10.0|^11",
        "illuminate/validation": "^10.0|^11"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mindshaker\\LaravelUikitServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Mindshaker\\": "app/"
        }
    }
}
